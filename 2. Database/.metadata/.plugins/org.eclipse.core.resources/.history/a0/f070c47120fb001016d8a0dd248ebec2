CREATE TABLE test_employees(
	te_employee_id INT,
	te_first_name varchar(50),
	te_last_name varchar(50),
	te_salary int,
	te_department varchar(50)
);

CREATE TABLE position1(
		position1_id int PRIMARY KEY AUTO_INCREMENT,
		position_name varchar(10)
);


CREATE TABLE employees1 (
	employees1_id int PRIMARY KEY AUTO_INCREMENT,
	name varchar(15),
	age int,
	gender varchar(1) check(gender IN('W', 'M')),
	dept varchar(15),
	salary int,
	position1_id int,
	CONSTRAINT fk_position FOREIGN KEY (position1_id)
	REFERENCES position1(position1_id)
	ON DELETE cascade
);

DROP TABLE employees1
SELECT * FROM employees1;

INSERT INTO ddl_alter values('qooo4051',
							'이영훈',
							'qwer1234!'
								);

