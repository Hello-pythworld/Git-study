UPDATE employees1
SET salary = 3500
WHERE employees1.name = '김민수';

DELETE FROM member_primary
WHERE member_primary_id = 1;
SELECT * FROM member_primary;

DELETE FROM employees1
WHERE employees1.name='김민수';

DROP TABLE employees1;

SELECT name FROM employees1
WHERE salary >= 3000 AND dept='개발팀';

CREATE TABLE employees1 e (
	employees1_id int PRIMARY KEY AUTO_INCREMENT,
	name varchar(15),
	age int,
	gender varchar(1) check(gender IN('W', 'M')),
	dept varchar(15),
	salary int,
	position1_id int,
	CONSTRAINT fk_position FOREIGN KEY (position1_id)
	REFERENCES position1(position1_id)
	ON DELETE cascade
);
SELECT name FROM employees1
WHERE dept='기획팀' or dept='개발팀';

SELECT name FROM employees1
WHERE name LIKE '김%';

SELECT e.name, p.position1_name
FROM position1
JOIN employees1
ON p.position1_id = e.employees.id;

